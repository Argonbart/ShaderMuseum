	// case 1
	if (p.x <= a.x) {
		return distance(a, p);
	}
	// case 2
	if (p.x > a.x && p.x < b.x) {
		return abs(p.y - a.y); 	// abs = Betrag
	}
	// case 3
	if (p.x >= b.x) {
		return distance(b, p);
	}
}"
